<?xml version="1.0" encoding="utf-8"?>
<!-- A minimal clock icon for the adaptive launcher icon.
     It features a white circular ring, a clock hand, and a small center dot. -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">

    <!-- Outer ring (clock face) -->
    <path
        android:fillColor="@android:color/transparent"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="8"
        android:pathData="M54,10 a44,44 0 1,1 0,88 a44,44 0 1,1 0,-88"/>

    <!-- Clock hand -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M54,54 L54,24"/>

    <!-- Center dot -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M52,54 a2,2 0 1,0 4,0 a2,2 0 1,0 -4,0"/>
</vector>
